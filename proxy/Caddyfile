{
	debug
	log {
		format console
		level INFO
		output stdout
	}
	auto_https off
}

:5555 {
	@private-ipv4 remote_ip $ALLOWED_IPS
	@denied not remote_ip private_ranges

	route /jsonapi {
		respond @denied 403 {
			close
		}
	}

	reverse_proxy localhost:8080

	log {
		format console
		level INFO
		output stdout
	}
}
